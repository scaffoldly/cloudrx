{
  "name": "cloudrx",
  "version": "0.1.0",
  "description": "RxJS extensions with cloud-backed subjects and observables for persistence and orchestration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint src tests integration-tests --ext .ts",
    "lint:fix": "eslint src tests integration-tests --ext .ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts integration-tests/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:all": "npm run test && npm run test:integration"
  },
  "keywords": [
    "rxjs",
    "reactive",
    "cloud",
    "persistence",
    "observables",
    "subjects",
    "typescript"
  ],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.823.0",
    "@aws-sdk/lib-dynamodb": "^3.823.0",
    "pino": "^9.7.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "glob": "^11.0.2",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "testcontainers": "^11.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
